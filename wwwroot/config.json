        //     "placeholderImage": "https://img.youtube.com/vi/NTtSM70rIvI/maxresdefault.jpg"
        // },

        // True to display in-app guides.
        // "showInAppGuides": false,
		"languageConfiguration" : {
			"enabled": false,
			"debug": false,
			"languages": {
				"en": "english"
			},
			"fallbackLanguage": "en"
		},
